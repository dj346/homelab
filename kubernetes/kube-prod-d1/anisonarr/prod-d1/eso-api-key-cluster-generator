apiVersion: generators.external-secrets.io/v1alpha1
kind: ClusterGenerator
metadata:
  name: anisonarr-eso-api-key-cluster-generator
  namespace: anisonarr-prod-d1
spec:
  kind: Password
  generator:
    passwordSpec:
      length: 20
      digits: 0
      symbols: 0
      noUpper: false
      allowRepeat: true
